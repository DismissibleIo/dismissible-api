{
  "name": "nestjs-dynamodb-storage",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dynamodb-storage/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/dynamodb-storage",
        "main": "libs/dynamodb-storage/src/index.ts",
        "tsConfig": "libs/dynamodb-storage/tsconfig.lib.json",
        "assets": [
          "libs/dynamodb-storage/package.json",
          "libs/dynamodb-storage/README.md",
          {
            "input": "libs/dynamodb-storage/bin",
            "glob": "**/*",
            "output": "bin"
          }
        ],
        "generatePackageJson": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/dynamodb-storage/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/dynamodb-storage"],
      "options": {
        "jestConfig": "libs/dynamodb-storage/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "dynamodb:setup": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node libs/dynamodb-storage/bin/dynamodb-setup.js"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish --access public",
        "cwd": "dist/libs/dynamodb-storage"
      }
    },
    "nx-release-publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish --access public",
        "cwd": "dist/libs/dynamodb-storage"
      }
    }
  }
}
