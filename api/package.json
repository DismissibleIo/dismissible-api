{
  "name": "@dismissible/nestjs-api",
  "version": "2.0.3",
  "description": "Dismissible API application",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "require": "./src/index.js",
      "types": "./src/index.d.ts"
    }
  },
  "files": [
    "src",
    "bin",
    "config",
    "README.md"
  ],
  "bin": {
    "dismissible-api": "./src/main.js",
    "dismissible-storage-setup": "./bin/dismissible-storage-setup.js"
  },
  "scripts": {
    "start": "node src/main.js",
    "storage:init": "npm run storage:postgres:init",
    "storage:postgres:init": "npx dismissible-prisma generate",
    "storage:setup": "npm run storage:setup:postgres && npm run storage:setup:dynamodb",
    "storage:setup:postgres": "npx dismissible-prisma migrate deploy",
    "storage:setup:dynamodb": "npx dismissible-dynamodb-setup"
  },
  "dependencies": {
    "@dismissible/nestjs-core": "^2.0.3",
    "@dismissible/nestjs-item": "^2.0.3",
    "@dismissible/nestjs-jwt-auth-hook": "^2.0.3",
    "@dismissible/nestjs-storage": "^2.0.3",
    "@dismissible/nestjs-postgres-storage": "^2.0.3",
    "@dismissible/nestjs-dynamodb-storage": "^2.0.3",
    "@dismissible/nestjs-logger": "^2.0.3",
    "@dismissible/nestjs-rate-limiter-hook": "^2.0.3",
    "@nestjs/common": "^11.1.11",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-fastify": "^11.1.11",
    "fastify": "^5.6.2",
    "@fastify/helmet": "^13.0.2",
    "@fastify/static": "^8.3.0",
    "@nestjs/swagger": "^11.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "nest-typed-config": "^2.10.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "keywords": [
    "nestjs",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DismissibleIo/dismissible-api"
  },
  "publishConfig": {
    "access": "public"
  }
}
